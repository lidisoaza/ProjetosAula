@model IEnumerable<GarageStore.Models.Message>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Messages)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateSend)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserId)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Messages)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateSend)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserId)
        </td>
    </tr>
}
</table>

